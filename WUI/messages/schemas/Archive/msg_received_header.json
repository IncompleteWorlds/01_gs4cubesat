{
    "name": "MsgReceivedHeader",
    "namespace": "com.incomplete.cubegs.common.avro.messages",
    "type": "record",
    "doc": "This message only contains the header of the received message in order to extract the identification of the message",
    "fields" : [
        {"name": "header", "type": {
                "name" : "header",
                "type": "record",
                "doc": "This message describes the header of a message",
                "fields" : [
                    { "name": "version", "type": "string", "doc": "Fix value; 1.0" },
                    { "name": "msg_id", "type": "string", "doc": "Unique identifier of the message" },
                    { "name": "msg_code", "type": "string", "doc": "code of the operation to be executed" },
                    { "name": "authentication_key", "type": "string", "doc": "Authentication code. Not used" },
                    { "name": "execution_id", "type": "string", "doc": "Unique execution identifier" },
                    { "name": "user_id", "type": "string", "doc": "Unique user identifier" }
                ]
            }, "doc": "Common messages header"}
    ]
}
